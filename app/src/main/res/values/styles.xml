<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="edit_text">
        <item name="android:ems">10</item>
        <item name="android:padding">12dp</item>
        <item name="android:background">@drawable/rounded_corners_et</item>
    </style>
    <style name="edit_text_white">
        <item name="android:ems">10</item>
        <item name="android:padding">12dp</item>
        <item name="android:background">@drawable/rounded_corners_et_white</item>
    </style>
    <style name="empty_view_txt">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="my_btn_style">
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:elevation">10dp</item>
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="android:background">@drawable/rounded_corners_btn</item>
    </style>
    <style name="address_reference_txt">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="status_tag_style">
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="type_txt_details">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">italic</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="type_server_txt" parent="type_txt_details">
        <item name="android:background">@drawable/rounded_corners_et</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">8dp</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>
</resources>